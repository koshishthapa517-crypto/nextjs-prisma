generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  username      String
  fullname      String
  email     String   @unique
  age       Int?
  profilepic    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}